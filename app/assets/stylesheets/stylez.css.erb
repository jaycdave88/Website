  html,body{
    margin:0 auto;
    text-align:center;
    background-color:#fff;
    font-family: helvetica;
  }
  /*-------------------------*/
  /*bullet removal*/
  /*-------------------------*/
  ul{
    list-style-type: none;
  }
  /*-------------------------*/
  /*load screen*/
  /*-------------------------*/
  #load_screen{
    background: #15191F;
    opacity: 1;
    position: fixed;
    z-index: 10;
    top: 0;
    width: 100%;
    height: 1600px;
  }
  #load_screen > #loading{
    color: #fff;
    width: 100%;
    height: 24px;
    margin: 100px auto;
  }
  /*-------------------------*/
  /*header*/
  /*-------------------------*/
  .big_head{
    background-color: #1E2629;
    width: 100%;
    height: auto;
    font-size: 50px;
    color: #fff;
    margin: 0 auto;
    padding-bottom: 20px;
    padding: 20px;
  }
  /*-------------------------*/
  /*jump links*/
  /*-------------------------*/

  body > div.contain-to-grid.sticky > nav a{
    background-color: yellow;
    font-size: 16px;
  }
  body > div.big_head > div > nav{
    width: 100%;
  }
  body > div.big_head > div > nav > ul > center > a:nth-child(1){
    color:white;
  }
  body > div.big_head > div > nav > ul > center > a:nth-child(1):hover{
    color:#b61924;
  }

  /*-------------------------*/
  /*carousel*/
  /*-------------------------*/
  #carousel{
    width: 50%;
    margin: 0 auto;
    background: #1E2629;
  }

  /*-------------------------*/
  /*nav bar*/
  /*-------------------------*/
  .nav_links{
    font-size: 21px;
  }

  .nav_links a{
    color: #fff; 
  }

  .nav_links a:hover{
    color:#BCBDC1;
  }

  a.buttons{
    border: white 1px solid;
    color:#fff;
    padding: 15px;
    font-size: 18px;
  }

  a.buttons:hover{
    color:#555555;
    background-color: #fff;
    border: #333333 3px solid;
    -webkit-transition: background 1s ease-out;
    -moz-transition: background 1s ease-out;
    -o-transition: background 1s ease-out;
    -transition: background 1s ease-out;
  }

  .line_center{
    margin: 0;
    padding: 0 10px;
    background:#fff;
    display: inline-block;
  }
  .line_centerBlk{
    margin: 0;
    padding: 0 10px;
    background:#1E2629;
    display: inline-block;
    color:#fff;
  }

  .headers:after{
    content:"";
    position: absolute;
    top: 50%;
    left: 0;
    right: 0;
    border-top: solid 1.5px #BCBDC1;
    z-index: -1;
  }

  .headers{
    text-align: center;
    position: relative;
    z-index: 2;
  }

  #headers{
    margin: 0% 15%
  }

  /*-------------------------*/
  /*Title colors*/
  /*-------------------------*/
  a{
    color:#1E2629 ;
  }

  a:hover{
    color:#e86256;
  }

  /*-------------------------*/
  /**/
  /*-------------------------*/
  #title > em > h2{
    color: #fff;
  }

  #headers > h2{
    top:5px;
  }

  body > div:nth-child(1) > p{
    margin: 20px auto;
  }

  body > div:nth-child(1) > div > nav{
    margin:  16px auto;
  }