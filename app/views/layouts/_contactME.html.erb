<div id="headers">
	<h2 class='headers'>
		<span class="line_center">
			<a name="contact_me" style="cursor:default">
				Contact Me
			</a>
		</span>
	</h2>
	<div id="flash_message"></div>

	<form role="form" action='/api/v1/contact' method='post' id="contactForm" data-remote=true>
		<input type="text" placeHolder="Enter Name" name="name" id="name">
		<input type="email" placeHolder="Enter Email" name="email" id="email">
		<input type="text" placeHolder="Subject" name="subject" id="subject">
		<textarea name='message' placeholder='Type your message here' id="message"></textarea>
		<input class="btn submit_contactMe" type="submit" value="Send Message">
	</form>
	
</div>